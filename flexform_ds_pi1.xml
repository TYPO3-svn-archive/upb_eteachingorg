                                        <size>1</size>
                                        <multiple>0</multiple>
                                        <selectedListStyle>width:150px</selectedListStyle>
                                        <itemListStyle>width:150px</itemListStyle>
                                </config>
                        </TCEforms>
                </what_to_display>
		<source>
                        <TCEforms>
                                <label>Quelle</label>
                                <config>
                                        <type>select</type>
                                        <items type="array">
                                        <numIndex index="0" type="array">
