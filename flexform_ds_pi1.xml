		 <maxListElements>
                                <TCEforms>
                                <label>Max. Anzahl von Listenelementen</label>
                                <config>
                                        <type>input</type>
                                        <eval>num</eval>
                                        <checkbox></checkbox>
                                        <size>3</size>
                                </config>
                                </TCEforms>
                 </maxListElements>
		<displayPageBrowser>
                        <TCEforms>
